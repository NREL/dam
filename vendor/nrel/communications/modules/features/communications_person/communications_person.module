<?php

/**
 * @file
 * Contains communications_person.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function communications_person_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the communications_person module.
    case 'help.page.communications_person':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Configurations for NREL people.') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function communications_person_theme() {
  return [
    'communications_person' => [
      'render element' => 'children',
    ],
  ];
}
