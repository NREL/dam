<?php

/**
 * @file
 * Contains communications_sso.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function communications_sso_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the communications_sso module.
    case 'help.page.communications_sso':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Enable NREL SSO.') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function communications_sso_theme() {
  return [
    'communications_sso' => [
      'render element' => 'children',
    ],
  ];
}
